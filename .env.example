# Purolator API Configuration

# Your Purolator API activation key
PUROLATOR_ACTIVATION_KEY=your_activation_key_here

# Your registered Purolator account number
PUROLATOR_ACCOUNT_NUMBER=your_account_number_here

# Environment: 'production' or 'development' (default: development)
PUROLATOR_ENVIRONMENT=development

# Language: 'en' or 'fr' (default: en)
PUROLATOR_LANGUAGE=en

# Group ID (optional, default: e-tracking)
PUROLATOR_GROUP_ID=e-tracking

# MongoDB Configuration

# MongoDB connection string
MONGODB_URI=mongodb://localhost:27017

# Database name (default: e-tracking)
MONGODB_DB_NAME=e-tracking
